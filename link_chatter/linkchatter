r = vector_store.as_retriever(search_kwargs={\"k\": 5})  # Retrieve more relevant results\n","    qa_chain = RetrievalQA.from_chain_type(llm, retriever=retriever)\n","\n","    try:\n","        answer = qa_chain.run(query)\n","        return answer if answer else \"I couldn't find an answer. Try rephrasing your question.\"\n","    except Exception as e:\n","        return f\"Error: {str(e)}\"\n","\n","# Create Gradio interface with error handling\n","def chatbot_interface(url, query):\n","    if not url.strip():\n","        return \"Error: Please enter a valid URL.\"\n","    if not query.strip():\n","        return \"Error: Please enter a question.\"\n","    return answer_query(url, query)\n","\n","iface = gr.Interface(\n","    fn=chatbot_interface,\n","    inputs=[gr.Textbox(label=\"Enter Website URL\"), gr.Textbox(label=\"Ask a Question\")],\n","    outputs=\"text\",\n","    title=\"Web Q&A Chatbot\",\n","    description=\"Enter a webpage URL and ask questions about its content.\"\n",")\n","\n","iface.launch()\n","\n","\n","\n","\n"],"metadata":{"id":"K40zUkWCerAx","executionInfo":{"status":"ok","timestamp":1743479564263,"user_tz":-330,"elapsed":7730,"user":{"displayName":"Harsh Mane","userId":"11188569698197169109"}},"outputId":"d9794ea0-0b13-4851-fad3-a9b717e2f877","colab":{"base_uri":"https://localhost:8080/","height":1000}},"execution_count":4,"outputs":[{"output_type":"stream","name":"stdout","text":["Requirement already satisfied: gradio in /usr/local/lib/python3.11/dist-packages (5.23.2)\n","Requirement already satisfied: sentence-transformers in /usr/local/lib/python3.11/dist-packages (3.4.1)\n","Requirement already satisfied: transformers in /usr/local/lib/python3.11/dist-packages (4.50.2)\n","Requirement already satisfied: faiss-cpu in /usr/local/lib/python3.11/dist-packages (1.10.0)\n","Requirement already satisfied: aiofiles<24.0,>=22.0 in /usr/local/lib/python3.11/dist-packages (from gradio) (23.2.1)\n","Requirement already satisfied: anyio<5.0,>=3.0 i